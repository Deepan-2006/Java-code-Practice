public class Logical{
    public static void main(String[] args) {
        System.out.println(5>4 && 8>7);
        System.err.println(5<4 && 8<7);
        System.out.println(5>4 || 8<7);
        System.out.println(5<4 || 8<7); 
        System.out.println(!(5>4));
        System.err.println(!(5<4));
    }
}
